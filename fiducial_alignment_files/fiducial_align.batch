#!/bin/bash

#SBATCH --ntasks=1
#SBATCH --mem=50G
#SBATCH --cpus-per-task=12
#SBATCH --time=02:00:00
#SBATCH --array=0-6

pwd; hostname; date
echo This is task $SLURM_ARRAY_TASK_ID

module load matlab/r2019a

source ~/miniconda3/bin/activate

conda activate python3.7

python fiducial_align_batch.py

date
